{
  "rewrites": [
    { "source": "/dashboard", "destination": "/dashboard.html" },
    { "source": "/leads", "destination": "/leads.html" },
    { "source": "/lead", "destination": "/lead.html" },
    { "source": "/pipeline", "destination": "/pipeline.html" },
    { "source": "/calendar", "destination": "/calendar.html" },
    { "source": "/offers", "destination": "/offers.html" },
    { "source": "/offer", "destination": "/offer.html" },
    { "source": "/settings", "destination": "/settings.html" },
    { "source": "/audit", "destination": "/audit.html" },
    { "source": "/p/:token", "destination": "/client-offer.html" },
    { "source": "/", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "DENY" }
      ]
    }
  ]
}
